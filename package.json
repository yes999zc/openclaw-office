{
  "name": "@ww-ai-lab/openclaw-office",
  "version": "0.1.1",
  "type": "module",
  "license": "MIT",
  "description": "Visual monitoring & management frontend for OpenClaw Multi-Agent system",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WW-AI-Lab/openclaw-office.git"
  },
  "homepage": "https://github.com/WW-AI-Lab/openclaw-office",
  "bugs": {
    "url": "https://github.com/WW-AI-Lab/openclaw-office/issues"
  },
  "keywords": [
    "openclaw",
    "multi-agent",
    "ai-agents",
    "office",
    "visualization",
    "monitoring",
    "dashboard",
    "react",
    "vite"
  ],
  "bin": {
    "openclaw-office": "bin/openclaw-office.js"
  },
  "files": [
    "dist/",
    "bin/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "start": "node ./bin/openclaw-office.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "oxlint src/",
    "format": "oxfmt --write src/",
    "format:check": "oxfmt --check src/",
    "check": "pnpm lint && pnpm format:check",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/postprocessing": "^3.0.4",
    "i18next": "^25.8.13",
    "i18next-browser-languagedetector": "^8.2.1",
    "immer": "^10.1.1",
    "lucide-react": "^0.575.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^16.5.4",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.1",
    "react-textarea-autosize": "^8.5.9",
    "recharts": "^2.15.0",
    "remark-gfm": "^4.0.1",
    "three": "^0.183.1",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@types/three": "^0.183.1",
    "@vitejs/plugin-react": "^4.5.0",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4.1.0",
    "typescript": "^5.8.0",
    "vite": "^6.3.0",
    "vitest": "^3.1.0"
  }
}
